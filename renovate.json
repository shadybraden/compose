{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "docker:enableMajor"
  ],
  "commitMessagePrefix": ":label:",
  "packageRules": [
    {
      "packagePatterns": ["^lscr.io\\/linuxserver\\/"],
      "versioning": "regex:^(?<major>\\d+)\\.(?<minor>\\d+)_p(?<patch>\\d+)-r(?<build>\\d)-ls(?<revision>\\d+)"
    }
  ]
}