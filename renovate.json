{
  "extends": ["config:base"],
  "regexManagers": [
    {
      "description": "Match searxng images in compose files, ignore Git hash",
      "fileMatch": [".*compose\\.ya?ml$"],
      "matchStrings": [
        "image:\\s*docker\\.io/searxng/searxng:(?<currentValue>\\d{4}\\.\\d{1,2}\\.\\d{1,2})-[a-zA-Z0-9]+"
      ],
      "datasourceTemplate": "docker",
      "packageNameTemplate": "searxng/searxng",
      "versioningTemplate": "loose",
      "debug": true
    }
  ],
  "commitMessagePrefix": ":label:"
}
